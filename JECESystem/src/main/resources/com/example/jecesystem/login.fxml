<VBox alignment="CENTER" prefHeight="600.0" prefWidth="1067.0" spacing="20.0" stylesheets="@login.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jecesystem.LoginController">
